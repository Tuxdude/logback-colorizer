<?xml version="1.0"?>
<FindBugsFilter>
    <Match>
        <Bug pattern="ST_WRITE_TO_STATIC_FROM_INSTANCE_METHOD" />
    </Match>
    <Match>
        <Class name="org.tuxdude.logback.extensions.LogColorizer" />
        <Or>
            <Field name="mColorError" />
            <Field name="mColorWarn" />
            <Field name="mColorInfo" />
            <Field name="mColorDebug" />
            <Field name="mColorTrace" />
            <Field name="mIsColorizerEnabled" />
            <Field name="mIsInitialized" />
        </Or>
        <Bug pattern="MS_PKGPROTECT" />
    </Match>
</FindBugsFilter>